digraph finite_state_machine {
	rankdir = LR;
	/*size = "4,6"*/
	/*ppi = 300*/
	node [ shape = none ] "";
	node [ shape = doublecircle ]; "1,2" "2,3";
	node [ shape = circle ];

	"" -> "1,2"

	"1,2" -> "3,1"  [ label = "a" ]
	"1,2" -> "∅"  [ label = "b" ]

	"∅" -> "∅" [ label = "a,b" ]

	"3,1" -> "2,3"  [ label = "a" ]
	"3,1" -> "2,3"  [ label = "b" ]

	"2,3" -> "1,2"  [ label = "a" ]
	"2,3" -> "2,3"  [ label = "b" ]
}
