digraph finite_state_machine {
	rankdir = LR;
	/*size = "4,6"*/
	/*ppi = 300*/
	node [ shape = none ] "";
	node [ shape = doublecircle ]; "2";
	node [ shape = circle ];

	"" -> "0"

	"0" -> "1"  [ label = "a" ]
	"0" -> "0"   [ label = "b" ]

	"1" -> "2"   [ label = "a" ]
	"1" -> "1"   [ label = "b" ]

	"2" -> "2" [ label = "a"]
	"2" -> "2" [ label = "b"]
}
