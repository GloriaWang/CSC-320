digraph finite_state_machine {
	rankdir = LR;
	/*size = "4,6"*/
	/*ppi = 300*/
	node [ shape = none ] "";
	node [ shape = doublecircle ];

	"" -> "(0,A)"

	"(0,A)" -> "(1,B)" [ label = "a" ]
	"(0,A)" -> "(0,A)" [ label = "b" ]
	"(0,B)" -> "(1,C)" [ label = "a" ]
	"(0,B)" -> "(0,B)" [ label = "b" ]
	"(0,C)" -> "(1,D)" [ label = "a" ]
	"(0,C)" -> "(0,C)" [ label = "b" ]
	"(0,D)" -> "(1,D)" [ label = "a" ]
	"(0,D)" -> "(0,D)" [ label = "b" ]

	"(1,A)" -> "(2,B)" [ label = "a" ]
	"(1,A)" -> "(1,A)" [ label = "b" ]
	"(1,B)" -> "(2,C)" [ label = "a" ]
	"(1,B)" -> "(1,B)" [ label = "b" ]
	"(1,C)" -> "(2,D)" [ label = "a" ]
	"(1,C)" -> "(1,C)" [ label = "b" ]
	"(1,D)" -> "(2,D)" [ label = "a" ]
	"(1,D)" -> "(1,D)" [ label = "b" ]

	"(2,A)" -> "(2,B)" [ label = "a" ]
	"(2,A)" -> "(2,A)" [ label = "b" ]
	"(2,B)" -> "(2,C)" [ label = "a" ]
	"(2,B)" -> "(2,B)" [ label = "b" ]
	"(2,C)" -> "(2,D)" [ label = "a" ]
	"(2,C)" -> "(2,C)" [ label = "b" ]
	"(2,D)" -> "(2,D)" [ label = "a" ]
	"(2,D)" -> "(2,D)" [ label = "b" ]
}
