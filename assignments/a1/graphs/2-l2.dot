digraph finite_state_machine {
	rankdir = LR;
	/*size = "4,6"*/
	/*ppi = 300*/
	node [ shape = none ] "";
	node [ shape = doublecircle ]; "A" "B" "C";
	node [ shape = circle ];

	"" -> "A"

	"A" -> "B"  [ label = "a" ]
	"A" -> "A"   [ label = "b" ]

	"B" -> "C"   [ label = "a" ]
	"B" -> "B"   [ label = "b" ]

	"C" -> "D" [ label = "a"]
	"C" -> "C" [ label = "b"]

	"D" -> "D" [ label = "a"]
	"D" -> "D" [ label = "b"]
}
